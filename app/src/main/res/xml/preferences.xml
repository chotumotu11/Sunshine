<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <EditTextPreference
        android:title="@string/preference_location"
        android:key="@string/preference_location_key"
        android:defaultValue="@string/preference_location_default"
        android:inputType="text"
        android:singleLine="true"/>

    <ListPreference
        android:key="@string/list_key"
        android:title="@string/list_title"
        android:defaultValue="@string/list_default"
        android:dialogTitle="@string/list_dialogtitle"
        android:entries="@array/List_entries"
        android:entryValues="@array/List_entriesValues"
        />

</PreferenceScreen>